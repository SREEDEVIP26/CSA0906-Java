import java.io.*;
import java.util.Scanner;
class Sree
{
    public static void main(String args[])
    {
        int a,r,sum=0;
        Scanner duo=new Scanner (System.in);
        System.out.println("a = ");
        a=duo.nextInt();
    
        while(a>0)
        {
        r=a%10;
        a=a/10;
        System.out.print(r);
        }
    }
}
